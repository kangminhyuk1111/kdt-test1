<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
  </head>
  <body>
    <form name="a">
      <input type="number" name="a" id="a" />
    </form>
    <button type="button" onclick="func1(4)">11</button>
    <form name="b">
      <input type="number" name="b" id="b" />
    </form>
    <button type="button" onclick="func2(5)">22</button>
    <script>
      const func1 = async (a) => {
        const data = await axios({
          url: "/function1",
          method: "GET",
          params: { a: a },
        });
      };
      const func2 = async (b) => {
        const data = await axios({
          url: "/function2",
          method: "GET",
          params: { b: b },
        });
      };
    </script>
  </body>
</html>
